#!/bin/bash

# GTK
exec_always     $HOME/.config/sway/bin/gsettings

# Bar
exec    $HOME/.config/sway/bin/bar

# Notfication
exec    $HOME/.config/sway/bin/dunst

# Autotiling (Not recommend when you are using eww or window rules)
# exec_always    autotiling

# Mpd
exec 	$HOME/.config/sway/bin/music
